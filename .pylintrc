[MASTER]
extension-pkg-whitelist=PyQt5


[REPORTS]
output-format=colorized

[MESSAGES CONTROL]
disable=
  line-too-long,
  missing-function-docstring,
  missing-class-docstring,
  missing-module-docstring,
  wildcard-import,
  unused-wildcard-import,
  too-few-public-methods,
  too-many-arguments,
  too-many-arguments,
  broad-except,
  redefined-outer-name,
